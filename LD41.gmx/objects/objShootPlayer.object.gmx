            <string>// Walk around

var walkSpeed = 3

var h = 0
if keyboard_check(vk_left) or keyboard_check(ord('A')) {
    h = -walkSpeed
} else if keyboard_check(vk_right) or keyboard_check(ord('D')) {
    h = walkSpeed
}
var v = 0
if keyboard_check(vk_up) or keyboard_check(ord('W')) {
    v = -walkSpeed
} else if keyboard_check(vk_down) or keyboard_check(ord('S')) {
    v = walkSpeed
}

x += h
y += v

// If we just walked into a wall, move back, and then rub up against walls
// in the x direction and the y direction.

if place_meeting(x, y, objWall) {
    x -= h
    y -= v
    
    for (var i=0; i&lt;abs(h); i++) {
        x += sign(h)
        if place_meeting(x, y, objWall) {
            x -= sign(h)
            break
        }
    }
    
    for (var i=0; i&lt;abs(v); i++) {
        y += sign(v)
        if place_meeting(x, y, objWall) {
            y -= sign(v)
            break
        }
    }
}

// Shoot bullets

if mouse_check_button(mb_left) {
    var shootDir = point_direction(x, y, mouse_x, mouse_y)
    with instance_create(x, y, objPlayerBullet) {
        direction = shootDir
        speed = 10
    }
}
</string>
